        <origin rpy="0 0 0" xyz="0 0 5"/>
        <axis xyz="0 0 1.0"/>
        <limit lower="-1.5708" upper="1.5708" velocity="1.0" effort="30" />
    </joint>

    <link name="robot3">
        <visual>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </collision>
    </link>

   <joint name="robot3_to_network" type="revolute">
